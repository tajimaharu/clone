<%= form_with(model: @clone, local: true) do |form| %>
  <div class="clone_title">
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>
  <div class="clone_content">
    <%= form.label :content %>
    <%= form.text_field :content %>
  </div>
  <%= form.submit value="Tweet"%>
<% end %>
